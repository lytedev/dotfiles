#!/usr/bin/env bash

rf="$1" # history count record file
cf="$1" # all choices file

touch "$LAUNCHER_HISTORY_FILE"
app="$(
	< "$rf" awk 'NF{NF--};1' | cat - "$cf" | \
		sort | uniq -c | sort -nr | \
		sd '^\s+' '' | \
		cut -d' ' -f2- | \
		sk
	)"
echo "$app $(date +%s)" >> "$LAUNCHER_HISTORY_FILE"
echo "$app"
